<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.maps.programacion.fundamentos.androidfundamentosproyecto">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="es.maps.programacion.fundamentos.es.androidfundamentosproyecto.application.MapsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${google_map_key}" />

        <uses-feature android:name="android.hardware.location.gps" />

        <activity android:name="es.maps.programacion.fundamentos.es.androidfundamentosproyecto.login.LoginActivity"></activity>
        <activity android:name="es.maps.programacion.fundamentos.es.androidfundamentosproyecto.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="es.maps.programacion.fundamentos.es.androidfundamentosproyecto.ui.ActividadMapa"
            android:label="@string/main_activity_titulo_pestaÃ±a_mapa"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>